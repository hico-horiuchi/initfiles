{
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-containers"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-containers"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "golang.go",
    "editor.indentSize": 4,
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.indentSize": 4,
    "editor.tabSize": 4
  },
  "[ruby]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "Shopify.ruby-lsp"
  },
  "[shellscript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.shellcheck": "explicit"
    },
    "editor.defaultFormatter": "timonwong.shellcheck"
  },
  "[terraform-vars]": {
    "editor.codeActionsOnSave": {
      "source.formatAll.terraform": "explicit"
    },
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[terraform]": {
    "editor.codeActionsOnSave": {
      "source.formatAll.terraform": "explicit"
    },
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "black-formatter.args": ["--line-length=120", "--target-version=py310"],
  "chat.instructionsFilesLocations": {
    ".github/instructions": true
  },
  "code-runner.enableAppInsights": false,
  "debug.internalConsoleOptions": "openOnSessionStart",
  "debug.openDebug": "openOnSessionStart",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "docker.extension.dockerEngineAvailabilityPrompt": false,
  "docker.extension.enableComposeLanguageServer": true,
  "docker.lsp.telemetry": "off",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "LigaRictyforPowerline-Regular",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.indentSize": 2,
  "editor.letterSpacing": 0.5,
  "editor.lineHeight": 1.5,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.semanticHighlighting.enabled": true,
  "editor.showFoldingControls": "always",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?ã€€ã€ã€‚ï¼Œï¼ãƒ»ï¼šï¼›ï¼Ÿï¼â€˜â€™â€œâ€ï¼ˆï¼‰ã€”ã€•ï¼»ï¼½ï½›ï½ã€ˆã€‰ã€Šã€‹ã€Œã€ã€ã€ã€ã€‘",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.incrementalNaming": "smart",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "**/defaults/*.yaml": "ansible",
    "**/defaults/*.yml": "ansible",
    "**/group_vars/*.yaml": "ansible",
    "**/group_vars/*.yml": "ansible",
    "**/handlers/*.yaml": "ansible",
    "**/handlers/*.yml": "ansible",
    "**/host_vars/*.yaml": "ansible",
    "**/host_vars/*.yml": "ansible",
    "**/meta/*.yaml": "ansible",
    "**/meta/*.yml": "ansible",
    "**/tasks/*.yaml": "ansible",
    "**/tasks/*.yml": "ansible",
    "**/vars/*.yaml": "ansible",
    "**/vars/*.yml": "ansible",
    "*.slm": "slim"
  },
  "files.autoGuessEncoding": true,
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.venv": true,
    "**/__pycache__": true,
    "**/node_modules": true,
    "**/vendor": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "flake8.args": [
    "--exclude=.git,.venv,plugin-builder/src/qmonus_sdk_plugins/libs/",
    "--ignore=B006,B007,B008,B011,B015,B018,B904,B905,E203,E226,E266,E303,E304,E402,E501,E741,F401,F403,F405,F541,F723,F821,F841,W503",
    "--max-line-length=120"
  ],
  "git.allowForcePush": true,
  "git.autoStash": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.ignoreLegacyWarning": true,
  "git.ignoreMissingGitWarning": true,
  "github.copilot.chat.localeOverride": "en",
  "github.copilot.nextEditSuggestions.enabled": true,
  "githubPullRequests.fileListLayout": "flat",
  "githubPullRequests.pullBranch": "never",
